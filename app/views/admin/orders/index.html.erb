<h1>注文履歴一覧</h1>
<table>
  <thead>
    <tr>
      <th>購入日時</th>
    <th>購入者</th>
    <th>注文個数</th>
    <th>注文ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td>
        <%= link_to admin_order_path(order.id) do %>
          <%= order.created_at %>
        <% end %>
      </td>
      <td><%= order.name %></td>
      <td><%#= order.order_details.amount %></td>
      <td><%= order.status %></td>
    </tr>
    <% end %>
  </tbody>
</table>