<div class="container">
    <div class="row">
      <h1>配送先編集</h1>
    </div>
    <%= form_with model: @address, url: public_address_path(@address.id), method: :patch do |f| %>
      <div class="row">
        <table class="table table-borderless">
          <tr>
            <td><%= f.label :郵便番号（ハイフンなし） %></td>
            <td><%= f.text_field :postal_code %></td>
          </tr>
          <tr>
            <td><%= f.label :住所 %></td>
            <td><%= f.text_field :addresses %></td>
          </tr>
          <tr>
            <td><%= f.label :宛名 %></td>
            <td><%= f.text_field :name %></td>
            <td><%= f.submit '変更内容を保存' %></td>
          </tr>
        </table>
      </div>
    <% end %>
  </div>

<!--form with url どこにデータを送るか-->
