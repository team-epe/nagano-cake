<h1>配送先編集</h1>

<%= form_with model: @address, url: '/public/addresses' do |f| %>
  <div class="form-group">
        <%= f.label :郵便番号（ハイフンなし） %>
        <%= f.text_field :postal_code %>
      </div>
      <div class="form-group">
        <%= f.label :住所 %>
        <%= f.text_field :address %>
      </div>
      <div class="form-group">
        <%= f.label :宛名 %>
        <%= f.text_field :name %>
      </div>
      <%= f.submit '変更内容を保存' %>
<% end %>